{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@components/*": ["components/*"],
      "@layouts/*": ["layouts/*"],
      "@pages/*": ["pages/*"],
      "@styles/*": ["styles/*"],
      "@icons/*": ["icons/*"],
      "@utils/*": ["utils/*"],
      "@content/*": ["content/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
